#!/usr/bin/env groovy

@Library('shared-library@master') _ //master or whatever branch

pipeline{

      
        
        stages{

              stage('maven build'){
                  steps{
                      script{
		    	                sh "mvn clean package"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 1)
                    }
                }
		
           }	       	     	         
}
